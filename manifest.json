{
  "name": "Hack Reactor Gmail Extension",
  "description": "getting started with searching emails for hiring partners",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": ["jquery.min.js", "inboxsdk.js","link-hiring-partners.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "manifest_version": 2
}
